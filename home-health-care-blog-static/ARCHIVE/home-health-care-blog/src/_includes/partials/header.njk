<header class="site-header" role="banner">
  <div class="container site-header__inner">
    <a class="site-logo" href="/">Home Health Care Services</a>
    <button class="nav-toggle" aria-expanded="false" aria-controls="primary-nav" data-nav-toggle>
      <span class="nav-toggle__label">Menu</span>
    </button>
    <nav id="primary-nav" class="site-nav" aria-label="Main">
      <ul>
        {% for item in nav %}
          <li>
            <a href="{{ item.url }}">{{ item.title }}</a>
          </li>
        {% endfor %}
      </ul>
    </nav>
    <button class="theme-toggle" type="button" data-theme-toggle aria-pressed="false">Toggle theme</button>
  </div>
</header>
